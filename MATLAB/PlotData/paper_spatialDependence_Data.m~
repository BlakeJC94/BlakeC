function paper_spatialDependence_Data

close all;
addpath(genpath('functions/'));

%% Get data path for constant model 
model = 0;
parameter = 0.5;

testoutput_path = ['PlotData/data/paper/spatialDependence/'...
    'spatialDependence_' num2str(model) '_testoutput_dats'];

data_path = [testoutput_path '/UtericBud_model_0_param_' ...
    num2str(parameter) '_pa_0_pd_0_simtime_600_'];

D = dir([data_path '*']);
TotalJobs = length(D(:));


for i = 1:TotalJobs
    
    loaddata = importdata([data_path 'sim_' num2str(i-1) ...
        '/results_from_time_zero/celltypescount.dat'])
    PopulationDataSet{i} = loaddata.data;
    
    if i == 1
        
        SimTime = PopulationDataSet{i}(:,1);
        
        TotalCells = [PopulationDataSet{i}(:,3) ...
            + PopulationDataSet{i}(:,2), ...
            zeros(length(SimTime), TotalJobs-1)];
        
        ProlifCells = [PopulationDataSet{i}(:,2), ...
            zeros(length(SimTime), TotalJobs-1)];
    else
        TotalCells(:, i) = PopulationDataSet{i}(:,3) + PopulationDataSet{i}(:,2);
        ProlifCells(:, i) = PopulationDataSet{i}(:,2);
    end
    
    
end



disp('pass!')
